{
  "cmd": "make",
  "cwd": "{PROJECT_PATH}/build/",
  "errorMatch" : "\n(?<file>[-\\\/0-9a-zA-Z\\._]+):(?<line>\\d+)(:(?<col>\\d+))",

  "targets": {
  "all": {
    "cmd": "make all",
    "cwd": "{PROJECT_PATH}/build/",
    "errorMatch" : "\n(?<file>[-\\\/0-9a-zA-Z\\._]+):(?<line>\\d+)(:(?<col>\\d+))"
    },
  "doc": {
    "cmd": "make doc",
    "cwd": "{PROJECT_PATH}/build/",
    "errorMatch" : "\n(?<file>[-\\\/0-9a-zA-Z\\._]+):(?<line>\\d+)(:(?<col>\\d+))?"
  },
  "clean": {
    "cmd": "make clean",
    "cwd": "{PROJECT_PATH}/build/"
  },
  "check": {
    "cmd": "make check",
    "cwd": "{PROJECT_PATH}/build/"
  },
  "rebuild_cache": {
    "cmd": "make rebuild_cache",
    "cwd": "{PROJECT_PATH}/build/"
    }
  }
}
